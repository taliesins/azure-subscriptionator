{
  "if": {
    "allOf": [
      {
        "field": "type",
        "equals": "Microsoft.Network/virtualNetworks/subnets"
      },
      {
        "not": {
          "allOf": [
            {
              "anyOf": [
                {
                  "field": "name",
                "match": "[uniqueString(resourceGroup().id)]-VN-.-SN-*"
                },
                {
                  "field": "name",
                "match": "[uniqueString(resourceGroup().id)]-VN-..-SN-*"
                },
                {
                  "field": "name",
                "match": "[uniqueString(resourceGroup().id)]-VN-...-SN-*"
                },
                {
                  "field": "name",
                "match": "[uniqueString(resourceGroup().id)]-VN-....-SN-*"
                },
                {
                  "field": "name",
                "match": "[uniqueString(resourceGroup().id)]-VN-.....-SN-*"
                },
                {
                  "field": "name",
                "match": "[uniqueString(resourceGroup().id)]-VN-......-SN-*"
                },
                {
                  "field": "name",
                "match": "[uniqueString(resourceGroup().id)]-VN-.......-SN-*"
                },
                {
                  "field": "name",
                "match": "[uniqueString(resourceGroup().id)]-VN-........-SN-*"
                }
              ]
            },
            {
              "anyOf": [
                {
                  "field": "name",
                  "match": "*-SN-."
                },
                {
                  "field": "name",
                  "match": "*-SN-.."
                },
                {
                  "field": "name",
                  "match": "*-SN-..."
                },
                {
                  "field": "name",
                  "match": "*-SN-...."
                },
                {
                  "field": "name",
                  "match": "*-SN-....."
                },
                {
                  "field": "name",
                  "match": "*-SN-......"
                },
                {
                  "field": "name",
                  "match": "*-SN-......."
                },
                {
                  "field": "name",
                  "match": "*-SN-........"
                }
              ]
            }
          ]
        }
      }
    ]
  },
  "then": {
  "effect": "[parameters('effect')]"
  }
}
